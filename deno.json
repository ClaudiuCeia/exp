{
  "name": "@claudiu-ceia/exp",
  "version": "0.1.0",
  "exports": {
    ".": "./mod.ts"
  },
  "publish": {
    "exclude": [
      "bench/",
      "tests/",
      "npm/"
    ]
  },
  "tasks": {
    "check": "deno fmt && deno lint && deno test --ignore=npm",
    "build:npm": "deno run -A scripts/build-npm.ts",
    "exp": "deno run -A cli/exp.ts",
    "repl": "deno task exp -- repl"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.17",
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@claudiu-ceia/combine": "jsr:@claudiu-ceia/combine@^0.2.8",
    "@stricli/core": "npm:@stricli/core@^1.2.5"
  }
}
